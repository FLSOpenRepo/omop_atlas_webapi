ClassMethod InsertLine(fileLine As %String, delimiter As %Char = "	") As %Status [ Language = objectscript ]
{
	s:$p(fileLine,delimiter,7)?4N1"-"2N1"-"2N $p(fileLine,delimiter,7) = $zdh($p($p(fileLine,delimiter,7), " ", 1),3)
	s:$p(fileLine,delimiter,8)?4N1"-"2N1"-"2N $p(fileLine,delimiter,8) = $zdh($p($p(fileLine,delimiter,8), " ", 1),3)

	set obj = ..%New()
	
	set obj.cdmsourcename = $p(fileLine, delimiter, 1)
	set obj.cdmsourceabbreviation = $p(fileLine, delimiter, 2)
	set obj.cdmholder = $p(fileLine, delimiter, 3)
	set obj.sourcedescription = $p(fileLine, delimiter, 4)
	set obj.sourcedocumentationreference = $p(fileLine, delimiter, 5)
	set obj.cdmetlreference = $p(fileLine, delimiter, 6)
	set obj.sourcereleasedate = $p(fileLine, delimiter, 7)
	set obj.cdmreleasedate = $p(fileLine, delimiter, 8)
	set obj.cdmversion = $p(fileLine, delimiter, 9)
	set obj.vocabularyversion = $p(fileLine, delimiter, 10)
	
	quit obj.%Save()
}

